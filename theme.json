{
  "version": 2,
  "settings": {
    "color": {
      "palette": [
        { "name": "Cream",        "slug": "cream",       "color": "#F5F0EB" },
        { "name": "Aqua",         "slug": "aqua",        "color": "#7FBAC8" },
        { "name": "Dark Green",   "slug": "dark-green",  "color": "#00202C" },
        { "name": "Dark",         "slug": "dark",        "color": "#2E2E2E" },
        { "name": "White",        "slug": "white",       "color": "#FFFFFF" }
      ]
    },
    "typography": {
      "fontFamilies": [
        {
          "name": "FreightNeo Pro",
          "slug": "freight-neo",
          "fontFamily": "'FreightNeo Pro', 'Cormorant Garamond', serif"
        },
        {
          "name": "Cormorant Garamond",
          "slug": "heading",
          "fontFamily": "Cormorant Garamond, serif",
          "fontFace": [
            {
              "fontFamily": "Cormorant Garamond",
              "fontWeight": "400 700",
              "fontStyle": "normal",
              "src": ["https://fonts.googleapis.com/css2?family=Cormorant+Garamond:wght@400;500;700&display=swap"]
            }
          ]
        },
        {
          "name": "Montserrat",
          "slug": "body",
          "fontFamily": "Montserrat, sans-serif",
          "fontFace": [
            {
              "fontFamily": "Montserrat",
              "fontWeight": "400 700",
              "fontStyle": "normal",
              "src": ["https://fonts.googleapis.com/css2?family=Montserrat:wght@400;700&display=swap"]
            }
          ]
        }
      ],
      "fontSizes": [
        { "name": "Small",    "slug": "small",    "size": "14px" },
        { "name": "Base",     "slug": "base",     "size": "16px" },
        { "name": "Medium",   "slug": "medium",   "size": "24px" },
        { "name": "Large",    "slug": "large",    "size": "40px" },
        { "name": "XLarge",   "slug": "xlarge",   "size": "68px" },
        { "name": "XXLarge",  "slug": "xxlarge",  "size": "96px" },
        { "name": "Hero",     "slug": "hero",     "size": "120px" }
      ]
    },
    "spacing": {
      "units": ["px", "%", "em", "rem", "vw"]
    },
    "layout": {
      "contentSize": "1200px",
      "wideSize": "1920px"
    }
  },
  "styles": {
    "color": {
      "background": "#f5f0eb",
      "text": "#2c2c2c"
    },
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--body)",
      "fontSize": "var(--wp--preset--font-size--base)",
      "lineHeight": "1.875"
    },
    "elements": {
      "h1": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--freight-neo)",
          "fontSize": "clamp(3.5rem, 8vw, 120px)",
          "fontWeight": "400",
          "lineHeight": "130px",
          "letterSpacing": "0",
          "textTransform": "uppercase"
        },
        "color": {
          "text": "var(--wp--preset--color--aqua)"
        }
      },
      "h2": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--body)",
          "fontSize": "clamp(1.5rem, 3vw, 40px)",
          "fontWeight": "700",
          "lineHeight": "100%",
          "textTransform": "uppercase"
        },
        "color": {
          "text": "#ffffff"
        }
      },
      "h3": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--heading)",
          "fontSize": "var(--wp--preset--font-size--xlarge)",
          "fontWeight": "400",
          "lineHeight": "1.09",
          "textTransform": "uppercase"
        }
      }
    }
  }
}